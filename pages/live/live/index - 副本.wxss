.wechat-container {
    background: #f4f4f4;
    height: 100%;
}

.line-video {
    height: 100%;
    overflow: hidden;
}
.line-video .video{
    width:100%;
    height:100%;
}

.pic-online {
    position: absolute;
    top:0;
    right:0;
    bottom:0;
    left:0;
}

.line-video image {
    width: 750rpx;
    height: 530rpx;
}

.line-op {
    width: 214rpx;
    height: 72rpx;
    background: #ebe9e8;
    padding: 10rpx;
    border-radius: 36rpx;
    position: absolute;
    left: 38rpx;
    top: 34rpx;
    /* overflow: hidden; */
    box-sizing: border-box;
    background: linear-gradient(to right, #f6a2b7, #f3cbc1);
}

.line-person {
    position: absolute;
    left: 300rpx;
    top: 38rpx;
}

.line-person image {
    width: 64rpx;
    height: 64rpx;
    border-radius: 32rpx;
    margin-right: 15rpx;
}

.line-person .delBtn {
    background: linear-gradient(to right, #f6a2b7, #f3cbc1);
    COLOR: #fff;
    float: right;
    height: 54rpx;
    width: 54rpx;
    border-radius: 27rpx;
    text-align: center;
    line-height: 54rpx;
    margin-top: 5rpx;
}

.line-op .line-pic {
    float: left;
}

.line-op .line-pic image {
    width: 54rpx;
    height: 54rpx;
    border-radius: 27rpx;
}

.line-op .line-info {
    margin-left: 12rpx;
    float: left;
}

.line-op .line-info .line-name {
    width: 115rpx;
    font-size: 25rpx;
    color: #333;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.line-op .line-info .line-num {
    font-size: 16rpx;
    color: #000;
}

.line-op .line-focus {
    float: right;
}

.line-op .line-focus .min-btn {
    width: 90rpx;
    height: 40rpx;
    border-radius: 20rpx;
    background: linear-gradient(to right, #f2cfc6, #f699b0);
    color: #fff;
    font-size: 25rpx;
    line-height: 40rpx;
    text-align: center;
    margin: 5rpx;
}

