/*商品列表  */

.shop-list {
    background: #fff;
}

.shop-list .title {
    padding: 14rpx 25rpx;
    background: #fff;
    overflow: hidden;
    border-bottom: 1rpx solid #f4f4f4;
}

.shop-list .title image {
    width: 23rpx;
    height: 23rpx;
    float: left;
    margin-top: 3rpx;
}

.shop-list .title .text {
    font-size: 25rpx;
    color: #000;
    padding: 0 10rpx;
    float: left;
}

.shop-list .title view:last-child {
    font-size: 25rpx;
    color: #f6819e;
    float: left;
}

.shop-list .shop-box .item {
    padding: 17px 24px;
    border-bottom: 1rpx solid #f4f4f4;
    overflow: hidden;
}

.shop-list .shop-box .item .pic {
    float: left;
    width: 148rpx;
    height: 148rpx;
    border: 1rpx solid #f4f4f4;
    position: relative;
    margin-right: 20rpx;
}

.shop-list .shop-box .item .pic image {
    width: 148rpx;
    height: 148rpx;
}

.shop-list .shop-box .item .pic view:first-child {
    font-size: 17rpx;
    color: #fff;
    background: #f6819e;
    width: 24rpx;
    height: 24rpx;
    position: absolute;
    top: 0;
    left: 0;
    text-align: center;
    line-height: 24rpx;
}

.shop-list .shop-box .item .text .shop-title {
    font-size: 25rpx;
    color: #000;
    line-height: 34rpx;
    margin-bottom: 35rpx;
}

.shop-list .shop-box .item .text .price view {
    color: #f6819e;
    font-size: 29rpx;
    line-height: 40rpx;
    float: left;
}

.shop-list .shop-box .item .text .price view:last-child {
    float: right;
    border: 1rpx solid #f6819e;
    width: 140rpx;
    height: 34rpx;
    border-radius: 17rpx;
    font-size: 25rpx;
    text-align: center;
    line-height: 34rpx;
}

/*加入购物车  */

.g-join {
    position: fixed;
    bottom: 0;
    left: 0;
    background: #fff;
    width: 750rpx;
    z-index: 5;
}

.g-join .delete {
    position: absolute;
    right: 10rpx;
    top: 10rpx;
}

.g-join .delete image {
    width: 40rpx;
    height: 40rpx;
}

.g-join .g-car-info {
    padding: 0 30rpx;
}

.g-join .pic-text {
    padding-bottom: 55rpx;
    height: 160rpx;
}

.g-join .pic-text image {
    width: 200rpx;
    height: 200rpx;
    float: left;
    margin: -20rpx 28rpx 0;
    border: 1rpx solid #efefef;
    border-radius: 5rpx;
}

.g-join .pic-text .text {
    float: left;
}

.g-join .pic-text .text .price {
    font-size: 41rpx;
    color: #f6819e;
    padding: 10rpx 0;
}

.g-join .pic-text .text .inv {
    color: #999;
    font-size: 25rpx;
    line-height: 35rpx;
    padding: 10rpx 0;
}

.g-join .pro-attr {
    border-top: 1rpx solid #e4e4e4;
    border-bottom: 1rpx solid #e4e4e4;
}

.g-join .pro-attr .title {
    color: #000;
    font-size: 25rpx;
    padding: 20rpx 0;
}

.g-join .pro-attr .pro-btn {
    overflow: hidden;
    padding-bottom: 20rpx;
}

.g-join .pro-attr .pro-btn view {
    float: left;
    height: 50rpx;
    width: 136rpx;
    text-align: center;
    line-height: 50rpx;
    border: 1rpx solid #f6819e;
    border-radius: 25rpx;
    color: #f6819e;
    margin-right: 24rpx;
    font-size: 25rpx;
}

.g-join .pro-attr .pro-btn .active {
    background: linear-gradient(#f69bb0, #facbc0);
    color: #fff;
}

.g-join .pro-num {
    margin: 20rpx 0;
}

.g-join .pro-num .text {
    float: left;
    color: #000;
    font-size: 25rpx;
}

.g-join .g-car-info .pro-num {
    padding: 20rpx 0 80rpx;
}

.g-join .g-car-info .pro-num .text {
    float: left;
    font-size: 25rpx;
    color: #000;
}

.g-join .g-car-info .pro-num .btn-num {
    float: right;
    background: #f4f4f4;
    height: 50rpx;
    width: 164rpx;
    border-radius: 25rpx;
    overflow: hidden;
    padding: 0 18rpx;
}

.g-join .g-car-info .pro-num .btn-num view {
    float: left;
    color: #f6819e;
    width: 55rpx;
    line-height: 36rpx;
    text-align: center;
    height: 36rpx;
    font-size: 45rpx;
    margin-top: 7rpx;
}

.g-join .g-car-info .pro-num .btn-num .num {
    background: #f6819e;
    color: #fff;
    border-radius: 5rpx;
    font-size: 25rpx;
}

.btn-group {
    display: flex;
    display: -webkit-flex;
    display: -webkit-box;
}

.btn-group view {
    flex: 1;
    -webkit-box-flex: 1;
    color: #fff;
    text-align: center;
    font-size: 29rpx;
    line-height: 85rpx;
    background: #ff658b;
}

.btn-group view:first-child {
    background: #ff9f85;
}

.boxBg {
    background: rgba(0, 0, 0, 0.3);
    position: fixed;
    bottom: 0;
    left: 0;
    width: 750rpx;
    height: 100%;
}
.dyn-op{box-sizing:border-box;padding:25rpx 40px;border-bottom:2rpx solid #f4f4f4;overflow:hidden;}
.dyn-op .dyn-step{float:right;margin-left:3px;}
.dyn-op .dyn-step button{border:0;background-color:transparent;}
.dyn-op .dyn-step image{width:56rpx;height:56rpx;margin-right:20rpx;}
.dyn-op .remark{
    float:left;
    position:relative;
}
.dyn-op .remark image{width:56rpx;height:56rpx;}
.dyn-op{
    position:absolute;
    width:100%;
    bottom:0;
}
.remark .num{
    position:absolute;
    top:-13rpx;
    right:-18rpx;
    width:52rpx;
    height:52rpx;
    text-align: center;
    line-height: 52rpx;
    color:#f6819e;
    background: #fff;
    font-size: 28rpx;
    border-radius: 52rpx;
    transform: scale(.5);
}
.popup .title{
    position:relative;
    padding-left:30rpx;
    line-height: 80rpx;
    border-bottom:1px solid #eee;
    font-size: 28rpx;
    font-weight: 600;
}
.popup .title .img{
    width:29rpx;
    height:29rpx;
}
.popup .title .num{
    color:#f6819e;
    font-size: 24rpx;
}
.popup .title .close{
    position:absolute;
    right:32rpx;
    top:39rpx;
    width:22rpx;
    height:22rpx;
}
.popup .con{
    width:100%;
    position: fixed;
    bottom:0;
    z-index: 10;
    background:  #fff;
    -webkit-transition-duration: .3s;
    transition-duration: .3s;
}
.popup.popup-enter .con,
.popup.popup-leave .con{
    -webkit-transform: translate3d(0, 100%, 0);
    transform: translate3d(0, 100%, 0);
}
.popup.popup-enter .mark{
    opacity: 0;
}
.scroll{
    height:600rpx;
}
.goods-item{
    position: relative;
    height:234rpx;
    padding:26rpx;
    border-radius: 8rpx;
    border-bottom:1rpx solid #f4f4f4;
}
.goods-img{
    float:left;
    width:180rpx;
    height:180rpx;
    margin-right:35rpx;
    background:#999;
}
.goods-name{
    min-height: 60rpx;
    margin-top:6rpx;
    font-size: 26rpx;
    font-weight: 600;
    line-height: 36rpx;
}
.goods-price{
    margin-top:80rpx;
    font-size: 22rpx;
    font-weight: 600;
    color:#f6819e;
}
.goods-sale{
    font-size: 22rpx;
    color:#999;
    margin-top:45rpx;
}
.goods-cart{
    position:absolute;
    right:32rpx;
    bottom:22rpx;
    width:190rpx;
    height:50rpx;
    font-size: 28rpx;
    color:#f6819e;
    text-align: center;
    line-height: 50rpx;
    border:1rpx solid #f6819e;
    border-radius: 50rpx;
}
.send-form .form{
    width:725rpx;
    margin:0 auto 14rpx;
    height:84rpx;
    line-height: 84rpx;
    border-radius: 8rpx;
    background: #fff;
}
.send-btn{
    width:135rpx;
    color:#f6819e;
}
.popup.send-form .con{
    background: none;
}
.send-form input{
    display: block;
    height: 84rpx;
