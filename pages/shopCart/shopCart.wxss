page{
    background: #f4f4f4;
}
.shopCart-page .page-content {
    margin-top: 2.2rem;
}

.shopCart-page .page-content .scroll-inner {
    padding-bottom: 5.2rem;
}

.shopCart-page .edit-bar {
    position: fixed;
    top: 0;
    width: 100%;
    height: 70rpx;
    padding:0 38rpx;
    background-color: #fff;
    border-top:1rpx solid #f4f4f4;
    z-index: 10;
    color:#666;
    font-size: 27rpx;
    line-height: 1;
    box-sizing: border-box;
    padding-top:17rpx;
}
.edit-bar .form-checkbox{
    margin-right:35rpx;
}
.shop-item{
    margin-bottom:13rpx;
}
.shop-store{
    position:relative;
    height:72rpx;
    font-size: 24rpx;
    box-sizing: border-box;
    line-height: 30rpx;
    padding-left:30rpx;
    padding-top:20rpx;
    background: #fff;
}
.shop-store .arrow{
    float:left;
    width:15rpx;
    height:15rpx;
    margin-top:8rpx;
    border-top:3rpx solid #cbcbcb;
    border-right:3rpx solid #cbcbcb;
    transform:rotate(45deg);
    margin-left:20rpx;
}
.shop-store .text{
    float:left;
}
.shop-store .form-checkbox{
    margin-right:35rpx;
}
.footer-box{
    height: 90rpx;
}
.shopCart-page .subfooter {
    position: fixed;
    margin: 0 auto;
    bottom: 0;
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    z-index: 5;
    background-color: #fff;
    font-size: 28rpx;
}

.shopCart-page .subfooter .bill-btn {
    width: 200rpx;
    height: 90rpx;
    text-align: center;
    border: 0;
    background-image: linear-gradient(90deg,#faccbf,#f69db1);
    border-radius: 0;
    color:#fff;
    font-size: 36rpx;
    line-height: 90rpx;
}

.shopCart-page .subfooter .bill-btn.on {
    color: #fff;
    background-color: #ec584e;
}

.shopCart-page .subfooter .totalMoney {
    text-align: right;
    margin-right:45rpx;
    font-size: 32rpx;
    color:#999;
}
.c58{
    color:#f4819e;
}
.shopCart-page .subfooter .form-checkbox {
    position: relative;
    top:7rpx;
}

.shopCart-page .item-cart {
    position: relative;
    padding-top: 27rpx;
    padding-bottom: 27rpx;
    border-top:2rpx solid #f4f4f4;
    background-color: #fff;
}

.shopCart-page .item-cart .check-container {
    float: left;
}
.item-cart .form-checkbox{
    width: 90rpx!important;
    height: 95rpx!important;
    padding-top: 60rpx;
    padding-left: 14rpx;
    padding-right:32rpx;
}
.shopCart-page .item-cart .check-container .check-state {
    display: inline-block;
    width: 2.0rem;
    height: 1.0rem;
    margin-left: 0.25rem;
    text-align: center;
    line-height: 1.0rem;
    color: #fff;
    border-radius: 0.15rem;
}

.shopCart-page .item-cart .check-container .check-state.failure {
    background-color: #aaa;
}

.shopCart-page .item-cart .check-failure {
    float: left;
    width: 2.0rem;
    padding: 0.25rem 0.5rem;
    margin-left: -0.5rem;
    color: #fff;
    background: #aaa;
    font-size: 0.6rem;
    border-radius: 3px;
}

.shopCart-page .item-cart .name-card {
    position: relative;
    overflow: hidden;
}

.shopCart-page .item-cart .thumb {
    float: left;
    width: 160rpx;
    height: 160rpx;
    border: 2rpx solid #f4f4f4;
    border-radius: 6rpx;
    margin-right: 20rpx;
}

.shopCart-page .item-cart .detail .p {
    position: relative;
    height: 0.8rem;
    padding-right: 1.0rem;
    margin: 0 0 0.1rem;
    font-size: 24rpx;
    line-height: 0.8rem;
    color: #666;
}

.shopCart-page .item-cart .detail .num {
    float: right;
    color: #8b8b8b;
    font-size: 24rpx;
}

.shopCart-page .item-cart .detail .price {
    position: absolute;
    left:185rpx;
    bottom:0;
    line-height: 1.5;
    font-weight: 700;
    color: #f6819e;
    font-size: 32rpx;
}

.shopCart-page .item-cart .detail .price-unit {
    font-size: 12px;
}

.shopCart-page .item-cart .detail .fail-tip {
    width: 9.0rem;
    height: 1.7rem;
    margin-right: 2.2rem;
}

.shopCart-page .item-cart .title {
    color:#000;
    line-height: 1.3;
    height: 75rpx;
    width:435rpx;
}

.shopCart-page .item-cart .delete-btn {
    position: absolute;
    right: 0;
    top: 0;
    width: 3.048rem;
    height: 100%;
    text-align: center;
    line-height: 7.0rem;
    color: #fff;
    background-color: #ff4343;
    font-size: 0.8rem;
    z-index: 2;
}

.shopCart-page .emptyList {
    padding: 1.5rem 0;
    background-color: #fff;
}

.shopCart-page .emptyList img {
    width: 6.0rem;
}

.shopCart-page .form-checkbox {
    display: inline-block;
    width: 34rpx;
    height: 34rpx;
    background-image: url(data:image/jpg;base64,);
    background-repeat: no-repeat;
    background-position: center;
    background-size: 34rpx 34rpx;
    border-radius: 34rpx;
}

.shopCart-page .form-checkbox.checked {
    background-image: url(data:image/jpg;base64,);
}

.shopCart-page .pageCon {
    height: 100%;
    background-color: #fff;
}

.shopCart-page .topfixed {
    position: fixed;
    width: 100%;
}

.shopCart-page .mui-number {
    position: absolute;
    bottom: 30rpx;
    right: 30rpx;
    width:205rpx;
    height: 60rpx;
    text-align: center;
    line-height: 60rpx;
    letter-spacing: 0;
    overflow: hidden;
    border-radius: 6rpx;
    border:2rpx solid #ccc;
    background-color: #fff;
}

.shopCart-page .mui-number.disabled {
    width: 6.1rem;
    height: 1.5rem;
    color: #fff;
    background-color: #aaa;
}

.shopCart-page .mui-number .decrease, .shopCart-page .mui-number .increase {
    position: relative;
    display: block;
    width:50rpx;
    height: 56rpx;
    color: #ccc;
}

.shopCart-page .mui-number .decrease {
    float: left;
    border-right: 2rpx solid #ccc;
}
.shopCart-page .mui-number .increase {
    float: right;
    border-left: 2rpx solid #ccc;
}
.shopCart-page .mui-number .decrease:before {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    content: '';
    width: 22rpx;
    height: 2rpx;
    background-color: #ccc;
}

.shopCart-page .mui-number .increase:before {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    content: '';
    width: 22rpx;
    height: 2rpx;
    background-color: #ccc;
}

.shopCart-page .mui-number .increase:after {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    content: '';
    width: 2rpx;
    height: 22rpx;
    background-color: #ccc;
}

.shopCart-page .mui-number .num {
    float: left;
    width: 98rpx;
    border: none;
    text-align: center;
    color: #424242;
    font-size: 24rpx;
    font-weight: 700;
}

.shopCart-page .fail-del-btn {
    display: block;
    width: 6.0rem;
    height: 1.5rem;
    margin: 0.5rem auto;
    text-align: center;
    line-height: 1.5rem;
    color: #ec584e;
    border: 1px solid #ec584e;
    border-radius: 0.15rem;
}
.footer-checkbox{
    padding-left:30rpx;
    color:#000;
}
.footer-checkbox .form-checkbox{
    margin-right:15rpx;
}

.empty{
    text-align: center;
    padding-top:130rpx;
}
.empty .txt {
    font-size: 32rpx;
    margin-bottom: 10rpx;
    color: #666;
}
.empty .subTxt {
    margin-bottom: 40rpx;
    color: #999;
}