.addrList-con .form-checkbox {
    border-radius: 100%;
    display: inline-block;
    vertical-align: middle;
    margin-right:15rpx;
    width: 30rpx;
    height: 30rpx;
    background-image: url(data:image/jpg;base64,);
    background-repeat: no-repeat;
    background-size: 100%;
}

.addrList-con .form-checkbox.checked {
    background-image: url(data:image/jpg;base64,);
}

.addrList-con .addNew {
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 83rpx;
    text-align: center;
    line-height: 83rpx;
    color: #fff;
    background-image: linear-gradient(90deg, #f69db1, #faccbf);
    font-size: 26rpx;
    z-index: 5;
    border: 0;
    margin: 0 auto;
    border-radius: 0;
}

.hr22 {
    line-height: 83rpx;
}

page {
    background: #f4f4f4;
    width:100%;
    font-size: 27rpx;
    color:#000;
}

.list-item {
    position:relative;
    min-height: 215rpx;
    margin-bottom: 10rpx;
    background: #fff;
    padding: 10rpx 33rpx 0 40rpx;
}
.list-item:before{
    position:absolute;
    content:"";
    right:45rpx;
    top:63rpx;
    width:14rpx;
    height:14rpx;
    border-top: 2rpx solid #d0d0d0;
    border-right:2rpx solid #d0d0d0;
    transform: rotate(45deg);
}
.list-item.default:after{
    position:absolute;
    content:"";
    left:0;
    bottom:0;
    height:8rpx;
    width:100%;
    background-image: linear-gradient(90deg, #f69db1, #faccbf);
}
.addr-detail{
    font-size: 22rpx;
    line-height: 42rpx;
}
.receiver-box{
    padding-right:57rpx;
    margin-top:20rpx;
    line-height: 54rpx;
}
.edit-line{
    line-height: 80rpx;
    height:80rpx;
    overflow: hidden;
    color:#999;
}
.edit-box{
    width:245rpx;
}
.edit{
    padding-left:40rpx;
    background-image: url(data:image/jpg;base64,);
    background-repeat: no-repeat;
    background-position: 0 28rpx;
    background-size: 29rpx 26rpx;
}
.delete{
    padding-left:30rpx;
    background-image: url(data:image/jpg;base64,);
    background-repeat: no-repeat;
    background-position: 0 28rpx;
    background-size: 20rpx 26rpx;
}
.empty{
    text-align: center;
    padding-top:130rpx;
}
.empty .txt {
    font-size: 32rpx;
    margin-bottom: 10rpx;
    color: #666;
}
.empty .subTxt {
    margin-bottom: 40rpx;
    color: #999;
}